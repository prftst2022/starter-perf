{
  "name": "starter-perf",
  "private": true,
  "workspaces": [
    "packages/*",
    "packages/emotion-components/*",
    "packages/scss-components/*"
  ],
  "description": "starter for test performance",
  "version": "0.0.0",
  "scripts": {
    "gatsby-emotion": "yarn workspace @starter-perf/gatsby-emotion develop",
    "gatsby-scss": "yarn workspace @starter-perf/gatsby-scss develop",
    "next-scss": "yarn workspace @starter-perf/next-scss develop",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "clean:all": "workspaces-foreach rimraf node_modules .next .cache && rimraf node_modules"
  },
  "dependencies": {},
  "devDependencies": {
    "prettier": "^2.6.2",
    "rimraf": "^3.0.2",
    "yarn-workspaces-foreach": "^0.2.0"
  },
  "repository": {
    "type": "git"
  }
}
